<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.practiceandroidjava">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PracticeAndroidJava">
        <activity
            android:name=".NumberActivity"
            android:label="@string/number" />
        <activity
            android:name=".PhraseActivity"
            android:label="@string/phrase"/>
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ColorActivity"
            android:label="@string/color"/>
        <activity
            android:name=".FaminlyActivy"
            android:label="@string/family"/>
    </application>

</manifest>